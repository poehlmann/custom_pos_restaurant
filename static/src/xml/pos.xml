<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="TableClockStart">
      <div class='control-button' style="width:100%; display: none;">
          <label>Entrada:</label>
        <div class="clock" id="clockStart"></div>
      </div>
  </t>
  <t t-name="TableAddCoupleButton">
     <div class='control-button'>
         Agregar Pareja
     </div>
  </t>
  <t t-name="TableUpdateOrderButton">
      <div class='control-button'>
         Calcular Total
      </div>
  </t>
  <t t-name="TableUpdateTimeButton">
      <div class='control-button' style="display: -webkit-inline-box;">
        <div class="reloj" id="Horas">00</div>
        <div class="reloj" id="Minutos">:00</div>
      </div>
  </t>
  <t t-name="TableUpdateTimeStopButton">
    <div class='control-button' style="display: -webkit-inline-box;">
        <div class="reloj" id="Detener">Detener</div>
    </div>
  </t>
</templates>
